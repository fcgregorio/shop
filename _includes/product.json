{
    "id": "{{ forloop.index }}",
    "url": "{{ product.url }}",
    "name": "{{ product.name }}",
    "brand": "{{ product.brand }}",
    "sku": "{{ product.sku }}",
    "price": "{{ product.price }}",
    "category": "{{ product.category }}",
    "subcategory": "{{ product.subcategory }}",
    "details": "{{ product.details }}",
    "images": "[{% for image in product.images %}{{ image }}{% unless forloop.last %}, {% endunless %}{% endfor %}]"
}